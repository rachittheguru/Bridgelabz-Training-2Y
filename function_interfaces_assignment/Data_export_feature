interface ReportExporter {
    void export(String data);

    default void exportToJSON(String data) {
        System.out.println("Exporting to JSON: { \"data\": \"" + data + "\" }");
    }
}

class CSVExporter implements ReportExporter {
    public void export(String data) {
        System.out.println("Exporting CSV: " + data);
    }
}

public class ExportSystem {
    public static void main(String[] args) {
        ReportExporter exporter = new CSVExporter();
        exporter.export("Report Data");
        exporter.exportToJSON("Report Data");
    }
}
